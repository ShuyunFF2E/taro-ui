{
  "name": "taro-ui",
  "version": "1.0.2-0",
  "private": true,
  "description": "智多星插件-多端组件库",
  "templateInfo": {
    "name": "mobx",
    "typescript": false,
    "css": "less"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:alipay": "taro build --type alipay",
    "build:h5": "taro build --type h5",
    "build:h5:mock": "taro build --type h5 --run mock ",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "build:component": "cross-env TARO_BUILD_TYPE=component taro build --ui",
    "version": "npm run build:h5 && npm publish dist"
  },
  "author": "shuyun-fe",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "^3.0.8",
    "@tarojs/components": "3.0.8",
    "@tarojs/react": "3.0.8",
    "@tarojs/runtime": "3.0.8",
    "@tarojs/taro": "3.0.8",
    "classnames": "^2.2.6",
    "lru-cache": "^6.0.0",
    "mobx": "4.8.0",
    "mobx-react": "6.1.4",
    "prop-types": "^15.7.2",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "taro-axios": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@tarojs/mini-runner": "3.0.8",
    "@tarojs/webpack-runner": "3.0.8",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^10.1.0",
    "babel-preset-taro": "3.0.8",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "dayjs": "^1.8.33",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-taro": "3.0.8",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "minimist": "^1.2.5",
    "nervjs": "^1.5.7",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "stylelint": "9.3.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "post-commmit": "git update-index --again"
    }
  },
  "lint-staged": {
    "src/**/*.{js,less}": "git add"
  }
}
